            data: $('#contactWithUs').serialize(), // Chuyển đổi dữ liệu biểu mẫu thành chuỗi
            success: function (response) {

                alert("Dữ liệu đã được gửi thành công ");
            },
            error: function (error) {
                // Xử lý trường hợp
                alert(error.responseText)

            }
        });
    });
